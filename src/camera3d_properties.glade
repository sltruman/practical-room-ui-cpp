<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <requires lib="gtk+" version="4.00" />
    <object class="GtkListBox" id="camera3d_properties">
        <property name="selection-mode">none</property>
        <property name="show-separators">1</property>
        <property name="width-request">400</property>
        <style>
            <class name="rich-list" />
        </style>
        <child>
            <object class="GtkListBoxRow">
                <property name="activatable">0</property>
                <child>
                    <object class="GtkLabel">
                        <property name="label">ActiveObject</property>
                        <attributes>
                            <attribute name="weight" value="PANGO_WEIGHT_BOLD" />
                        </attributes>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkListBoxRow">
                <property name="activatable">0</property>
                <child>
                    <object class="GtkBox">
                        <property name="homogeneous">1</property>
                        <child>
                            <object class="GtkLabel">
                                <property name="label">Name</property>
                                <property name="halign">start</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkLabel" id="name">
                                <property name="label">camera</property>
                                <property name="halign">end</property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkListBoxRow">
                <property name="activatable">0</property>
                <child>
                    <object class="GtkBox">
                        <property name="homogeneous">1</property>
                        <child>
                            <object class="GtkLabel">
                                <property name="label">Base</property>
                                <property name="halign">start</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkDropDown" id="base">
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkListBoxRow">
                <property name="activatable">0</property>
                <child>
                    <object class="GtkBox">
                        <property name="homogeneous">1</property>
                        <child>
                            <object class="GtkLabel">
                                <property name="label">X</property>
                                <property name="halign">start</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkSpinButton" id="x">
                                <property name="digits">2</property>
                                <property name="numeric">True</property>
                                <property name="adjustment">
                                    <object class="GtkAdjustment">
                                        <property name="upper">100</property>
                                        <property name="lower">-100</property>
                                        <property name="step-increment">0.01</property>
                                    </object>
                                </property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkListBoxRow">
                <property name="activatable">0</property>
                
                <child>
                    <object class="GtkBox">
                        <property name="homogeneous">1</property>
                        <child>
                            <object class="GtkLabel">
                                <property name="label">Y</property>
                                <property name="halign">start</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkSpinButton" id="y">
                                <property name="digits">2</property>
                                <property name="numeric">True</property>
                                <property name="adjustment">
                                    <object class="GtkAdjustment">
                                        <property name="upper">100</property>
                                        <property name="lower">-100</property>
                                        <property name="step-increment">0.01</property>
                                    </object>
                                </property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkListBoxRow">
                <property name="activatable">0</property>
                <child>
                    <object class="GtkBox">
                        <property name="homogeneous">1</property>
                        <child>
                            <object class="GtkLabel">
                                <property name="label">Z</property>
                                <property name="halign">start</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkSpinButton" id="z">
                                <property name="digits">2</property>
                                <property name="numeric">True</property>
                                <property name="adjustment">
                                    <object class="GtkAdjustment">
                                        <property name="upper">100</property>
                                        <property name="lower">-100</property>
                                        <property name="step-increment">0.01</property>
                                    </object>
                                </property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkListBoxRow">
                <property name="activatable">0</property>
                <child>
                    <object class="GtkBox">
                        <property name="homogeneous">1</property>
                        <child>
                            <object class="GtkLabel">
                                <property name="label">RX</property>
                                <property name="halign">start</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkSpinButton" id="rx">
                                <property name="digits">0</property>
                                <property name="numeric">True</property>
                                <property name="adjustment">
                                    <object class="GtkAdjustment">
                                        <property name="upper">180</property>
                                        <property name="lower">-180</property>
                                        <property name="step-increment">1</property>
                                    </object>
                                </property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkListBoxRow">
                <property name="activatable">0</property>
                <child>
                    <object class="GtkBox">
                        <property name="homogeneous">1</property>
                        <child>
                            <object class="GtkLabel">
                                <property name="label">RY</property>
                                <property name="halign">start</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkSpinButton" id="ry">
                                <property name="digits">0</property>
                                <property name="numeric">True</property>
                                <property name="adjustment">
                                    <object class="GtkAdjustment">
                                        <property name="upper">180</property>
                                        <property name="lower">-180</property>
                                        <property name="step-increment">1</property>
                                    </object>
                                </property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>

        <child>
            <object class="GtkListBoxRow">
                <property name="activatable">0</property>
                <child>
                    <object class="GtkBox">
                        <property name="homogeneous">1</property>
                        <child>
                            <object class="GtkLabel">
                                <property name="label">RZ</property>
                                <property name="halign">start</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkSpinButton" id="rz">
                                <property name="digits">0</property>
                                <property name="numeric">True</property>
                                <property name="adjustment">
                                    <object class="GtkAdjustment">
                                        <property name="upper">180</property>
                                        <property name="lower">-180</property>
                                        <property name="step-increment">1</property>
                                    </object>
                                </property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>

        <child>
            <object class="GtkListBoxRow">
                <property name="activatable">0</property>
                <child>
                    <object class="GtkLabel" id="kind">
                        <property name="label">Camera3D</property>
                        <attributes>
                            <attribute name="weight" value="PANGO_WEIGHT_BOLD" />
                        </attributes>
                    </object>
                </child>
            </object>
        </child>
                
        <child>
            <object class="GtkListBoxRow" id="row_height">
                <property name="activatable">0</property>
                <child>
                    <object class="GtkBox">
                        <property name="homogeneous">1</property>
                        <child>
                            <object class="GtkLabel">
                                <property name="label">Focal</property>
                                <property name="halign">start</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkSpinButton" id="focal">
                                <property name="digits">3</property>
                                <property name="adjustment">
                                    <object class="GtkAdjustment">
                                        <property name="upper">1</property>
                                        <property name="lower">0.001</property>
                                        <property name="value">0.01</property>
                                        <property name="step-increment">0.001</property>
                                    </object>
                                </property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkListBoxRow" id="row_rtt">
                <property name="activatable">0</property>
                <child>
                    <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <child>
                            <object class="GtkBox" id="box_capture">
                                <property name="orientation">vertical</property>
                                <child>
                                    <object class="GtkButton" id="capture">
                                        <property name="label">capture</property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkLabel" id="save">
                                        <property name="label"></property>
                                        <property name="wrap">True</property>
                                    </object>
                                </child>
                            </object>
                        </child>
                        <child>
                            <object class="GtkPicture" id="texture">
                                <property name="content-fit">contain</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkPicture" id="texture_depth">
                                <property name="content-fit">contain</property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>